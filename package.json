{
  "name": "nuxt-express-boilerplate",
  "version": "",
  "description": "",
  "author": "",
  "private": true,
  "engines": {
    "node": "^13.0.0",
    "npm": "^6.0.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node index.js",
    "lint": "npm-run-all --silent lint:*",
    "lint:eslint": "eslint .",
    "lint:stylelint": "stylelint ./client/**/*.scss",
    "lint-fix": "npm-run-all --continue-on-error --silent lint-fix:*",
    "lint-fix:eslint": "eslint --fix .",
    "lint-fix:stylelint": "stylelint --fix ./client/**/*.scss",
    "clean": "rm -rf .nuxt dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-fix"
    }
  },
  "dependencies": {
    "nuxt": "2.13.2",
    "cross-env": "7.0.2",
    "express": "4.17.1",
    "@nuxtjs/axios": "5.11.0",
    "bem-cn-lite": "4.0.0"
  },
  "devDependencies": {
    "npm-run-all": "4.1.5",
    "babel-eslint": "10.1.0",
    "eslint": "7.3.1",
    "eslint-plugin-nuxt": "1.0.0",
    "eslint-plugin-vue": "6.2.2",
    "eslint-plugin-node": "11.1.0",
    "husky": "4.2.5",
    "nodemon": "2.0.4",
    "sass-loader": "8.0.2",
    "node-sass": "7.0.0",
    "stylelint": "13.6.0",
    "stylelint-order": "4.1.0"
  }
}
